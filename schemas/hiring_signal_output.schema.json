{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "HiringSignalOutput",
  "type": "object",
  "required": [
    "run_id",
    "tool_version",
    "git_sha",
    "crawl_ts",
    "station",
    "max_distance_km",
    "business_id",
    "name",
    "domain",
    "signal",
    "confidence",
    "evidence",
    "evidence_snippets",
    "evidence_urls",
    "signal_url",
    "checked_urls",
    "next_url_hint",
    "errors",
    "skipped_reason",
    "ollama_model",
    "ollama_temperature",
    "prompt_version",
    "llm_used",
    "output_format"
  ],
  "properties": {
    "run_id": {"type": "string"},
    "tool_version": {"type": "string"},
    "git_sha": {"type": "string"},
    "crawl_ts": {"type": "string"},
    "station": {"type": "string"},
    "max_distance_km": {"type": "number"},
    "business_id": {"type": "string"},
    "name": {"type": "string"},
    "domain": {"type": "string"},
    "signal": {"type": "string", "enum": ["yes", "no", "unclear"]},
    "confidence": {"type": "number"},
    "evidence": {"type": "string"},
    "evidence_snippets": {"type": "array", "items": {"type": "string"}},
    "evidence_urls": {"type": "array", "items": {"type": "string"}},
    "signal_url": {"type": "string"},
    "checked_urls": {"type": "string"},
    "next_url_hint": {"type": "string"},
    "errors": {"type": "string"},
    "skipped_reason": {"type": "string"},
    "ollama_model": {"type": "string"},
    "ollama_temperature": {"type": "number"},
    "prompt_version": {"type": "string"},
    "llm_used": {"type": "boolean"},
    "output_format": {"type": "string", "enum": ["csv", "jsonl"]}
  }
}
